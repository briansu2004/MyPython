{
    "version": "3",
    "services": {
        "redis": {
            "container_name": "ex1_redis",
            "image": "redis:3.2.12",
            "volumes": [
                "./redis:/data"
            ],
            "restart": "always"
        },
        "mysql": {
            "container_name": "ex1_mysql",
            "image": "mysql:5.6",
            "environment": {
                "MYSQL_ROOT_PASSWORD": "root"
            },
            "volumes": [
                "./mysql:/var/lib/mysql"
            ],
            "restart": "always"
        }
    }
}
